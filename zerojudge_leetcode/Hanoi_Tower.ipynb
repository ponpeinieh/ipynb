    "def tower(n, source, buffer, target):\n",
    "    if n==1:\n",
    "        print(f'Move {n} from {source} to {target}')\n",
    "    else:\n",
    "        tower(n-1, source,target,buffer)\n",
    "        print(f'Move {n} from {source} to {target}')\n",
    "        tower(n-1, buffer,source,target)\n",
    "\n",
    "tower(4, 'A','B','C')\n"
   ]
  }
